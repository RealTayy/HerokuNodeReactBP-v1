{
  "name": "HerokuNodeReactBP-v1",
  "version": "1.0.0",
  "description": "Heroku App BP",
  "engines": {
    "node": "12.16.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "per-env",
    "start:development": "concurrently \"npm run client\" \"npm run server\"",
    "start:production": "npm run server",
    "install": "concurrently \"cd client && npm install\" \"cd server && npm install -s\"",
    "client": "per-env",
    "client:development": "cd client && npm run start",
    "client:production": "cd client && npm run start",
    "server": "per-env",
    "server:development": "cd server && npm run start",
    "server:production": "cd server && npm run start",
    "heroku-postbuild": "npm run build",
    "build": "concurrently \"cd client && npm run build\" \"cd server && npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RealTayy/HerokuNodeReactBP-v1.git"
  },
  "keywords": [],
  "author": "RealTayy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RealTayy/HerokuNodeReactBP-v1/issues"
  },
  "homepage": "https://github.com/RealTayy/HerokuNodeReactBP-v1#readme",
  "dependencies": {
    "per-env": "^1.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
